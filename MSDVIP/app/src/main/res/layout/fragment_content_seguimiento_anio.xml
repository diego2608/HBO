<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" android:layout_height="match_parent">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="350dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:weightSum="1"
        android:id="@+id/relativeLayout3"
        android:background="@drawable/imganio">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="Pacientes enviados:"
            android:id="@+id/textView39"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_marginLeft="20dp"
            android:layout_below="@+id/lblAnioActual" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="300"
            android:id="@+id/lblCantPacientesAÃ±o"
            android:layout_above="@+id/relativeLayout7"
            android:layout_toRightOf="@+id/lblAnioActual"
            android:layout_toEndOf="@+id/lblAnioActual" />

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_below="@+id/textView39"
            android:id="@+id/relativeLayout7"
            android:layout_alignParentBottom="true">

            <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="2dp"
                android:background="#bababa"
                android:id="@+id/linearLayout15"
                android:layout_alignParentTop="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_marginTop="20dp" />

            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/relativeLayout4"
                android:layout_below="@+id/linearLayout15"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true">

                <ProgressBar
                    style="?android:attr/progressBarStyleHorizontal"
                    android:id="@+id/progressBarAfiliadosAnio"
                    android:layout_width="62dp"
                    android:layout_height="62dp"
                    android:indeterminate="false"
                    android:max="10000"
                    android:progressDrawable="@drawable/circular_progress_potencial"
                    android:layout_gravity="center_horizontal"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="5dp"
                    android:layout_marginRight="10dp" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="70%"
                    android:id="@+id/lblPorcentajeAfiliadosAnio"
                    android:textSize="18dp"
                    android:textColor="@android:color/black"
                    android:layout_centerVertical="true"
                    android:layout_centerHorizontal="true" />
            </RelativeLayout>

            <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="2dp"
                android:background="#bababa"
                android:id="@+id/linearLayout16"
                android:layout_below="@+id/relativeLayout4"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_marginTop="5dp" />

            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_marginTop="5dp"
                android:layout_marginLeft="10dp"
                android:id="@+id/relativeLayout5">

                <ProgressBar
                    style="?android:attr/progressBarStyleHorizontal"
                    android:id="@+id/progressBarPendientesAnio"
                    android:layout_width="62dp"
                    android:layout_height="62dp"
                    android:layout_centerHorizontal="true"
                    android:max="10000"
                    android:progressDrawable="@drawable/circular_progress_afiliado"
                    android:layout_gravity="center_horizontal"
                    android:indeterminate="false" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="18%"
                    android:id="@+id/lblPorcentajePendientesAnio"
                    android:textSize="18dp"
                    android:textColor="@android:color/black"
                    android:layout_centerVertical="true"
                    android:layout_centerHorizontal="true" />
            </RelativeLayout>

            <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="2dp"
                android:background="#bababa"
                android:id="@+id/linearLayout17"
                android:layout_below="@+id/relativeLayout5"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_marginTop="5dp" />

            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/linearLayout17"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="5dp"
                android:id="@+id/relativeLayout6">

                <ProgressBar
                    style="?android:attr/progressBarStyleHorizontal"
                    android:id="@+id/progressBarNoAfiliadosAnio"
                    android:layout_width="62dp"
                    android:layout_height="62dp"
                    android:max="10000"
                    android:progressDrawable="@drawable/circular_progress_noafiliado"
                    android:layout_gravity="center_horizontal"
                    android:layout_centerHorizontal="true"
                    android:indeterminate="false" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="08%"
                    android:id="@+id/lblPorcentajeNoAfiliadosAnio"
                    android:textSize="18dp"
                    android:textColor="@android:color/black"
                    android:layout_centerVertical="true"
                    android:layout_centerHorizontal="true" />
            </RelativeLayout>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="210"
                android:id="@+id/lblCantidadAfiliadosAnio"
                android:layout_below="@+id/linearLayout15"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="20dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="pacientes Afiliados"
                android:id="@+id/textView44"
                android:textSize="16sp"
                android:layout_alignTop="@+id/lblCantidadAfiliadosAnio"
                android:layout_toRightOf="@+id/lblCantidadAfiliadosAnio"
                android:layout_toEndOf="@+id/lblCantidadAfiliadosAnio"
                android:layout_marginLeft="10dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="54"
                android:id="@+id/lblCantPendientesAnio"
                android:layout_alignTop="@+id/textView46"
                android:layout_alignLeft="@+id/lblCantidadAfiliadosAnio"
                android:layout_alignStart="@+id/lblCantidadAfiliadosAnio" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="pacientes pendientes"
                android:id="@+id/textView46"
                android:textSize="16sp"
                android:layout_centerVertical="true"
                android:layout_alignLeft="@+id/textView44"
                android:layout_alignStart="@+id/textView44" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="24"
                android:id="@+id/lblCantNoAfiliadosAnio"
                android:layout_alignTop="@+id/relativeLayout6"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="20dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:text="pacientes No Afiliados"
                android:id="@+id/textView48"
                android:textSize="16sp"
                android:layout_alignBottom="@+id/lblCantNoAfiliadosAnio"
                android:layout_alignLeft="@+id/textView46"
                android:layout_alignStart="@+id/textView46" />

        </RelativeLayout>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="     2015     "
            android:textSize="23sp"
            android:textColor="#FFFFFF"
            android:background="@drawable/lblshapeanio"
            android:id="@+id/lblAnioActual"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="15dp"
            android:layout_marginBottom="15dp"
            android:textStyle="bold"
            android:typeface="normal"
            android:layout_alignParentTop="false" />

    </RelativeLayout>


        <org.eazegraph.lib.charts.StackedBarChart
            xmlns:app="http://schemas.android.com/apk/res-auto"
            android:id="@+id/stackedbarchart"
            android:layout_width="match_parent"
            android:layout_height="256dp"
            android:padding="10dp"
            app:egFixedBarWidth="true"
            app:egBarWidth="20dp"
            app:egLegendHeight="40dp"
            android:layout_below="@+id/relativeLayout3"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

</RelativeLayout>